<map id="map" scale="{{scale}}" latitude="{{centerY}}" longitude="{{centerX}}"  markers="{{markers}}" bindmarkertap="markertap" bindregionchange="regionchange" show-location style="width: 100%;height:680px;z-index:8;">
        <cover-view bindtap="__getLocation" class="getLocation white-bg">
            <cover-view class="border">
                <cover-view class="point blue-bg"></cover-view>
            </cover-view>
        </cover-view>
        <cover-view class="markTip" style="top:226rpx">
            <cover-view>
                <cover-view class="markTipTx" style="background: #00c2a8"></cover-view>
                <cover-view class="markTipText fz-12">优秀</cover-view>
            </cover-view>
            <cover-view>
                <cover-view class="markTipTx" style="background: #00aeff"></cover-view>
                <cover-view class="markTipText fz-12">良好</cover-view>
            </cover-view>
            <cover-view>
                <cover-view class="markTipTx" style="background:#e9f812"></cover-view>
                <cover-view class="markTipText fz-12">一般</cover-view>
            </cover-view>
            <cover-view>
                <cover-view class="markTipTx" style="background: #fa543e"></cover-view>
                <cover-view class="markTipText fz-12">较差</cover-view>
            </cover-view>
            <cover-view class="back-home" wx:if="{{showHome}}">
                <cover-view bindtap="backHome" class="navigator">
                    <cover-view class="backHome fz-12 white-color">返回</cover-view>
                    <cover-view class="backHome fz-12 white-color">首页</cover-view>
                </cover-view>
            </cover-view>
        </cover-view>
    </map>
